{
  "author": "k2snowman69",
  "bin": {
    "exec-if-exists": "./dist/cli.js"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "npm-which": "^3.0.1"
  },
  "description": "Runs a npm cli package if it exists",
  "devDependencies": {
    "@snowcoders/renovate-config": "0.2.1",
    "@snowcoders/sortier": "2.5.1",
    "@types/node": "10.12.24",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "typescript": "3.3.3333"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "exec-if-exists",
  "repository": "github:k2snowman69/exec-if-exists",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "prettier": "prettier \"./src/index.ts\"",
    "start-fail": "npm run build && node ./dist/cli.js not_prettier \"./src/index.ts\"",
    "start-success": "npm run build && node ./dist/cli.js prettier \"./src/index.ts\""
  },
  "version": "1.0.1"
}
