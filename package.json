{
  "name": "exec-if-exists",
  "version": "1.0.0",
  "description": "Runs a npm cli package if it exists",
  "main": "./dist/index.js",
  "bin": {
    "exec-if-exists": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build",
    "start": "npm run build && node ./dist/cli.js prettier \"./src/index.ts\"",
    "prettier": "prettier \"./src/index.ts\""
  },
  "repository": "github:snowcoders/renovate-config",
  "author": "k2snowman69",
  "license": "MIT",
  "dependencies": {
    "@snowcoders/renovate-config": "0.1.3",
    "@snowcoders/sortier": "2.3.2",
    "execa": "^1.0.0",
    "husky": "1.2.1",
    "lint-staged": "8.1.0",
    "npm-which": "^3.0.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@types/node": "^10.12.15"
  }
}
